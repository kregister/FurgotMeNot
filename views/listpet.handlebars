        <h1 class="adoptions3">Pet Information</h1>
      </div>
      <br>
      <form  action="/postpet" method="POST">

        <div class="form-group row">
          <div class="col-sm-10">
            <label class="col-sm-6" for="house">Type of Pet:</label>
            <select name="genus" id="genus" class="col-sm-5">
              <option value="0"> Please Select </option>
              <option value="Dog"> Dog </option>
              <option value="Cat"> Cat </option>
              <!-- <option value="Bird">Bird</option>
              <option value="Rodent">Rodent</option> -->
            </select>
          </div>
        </div>

      <div class="form-group row">
        <div class="col-md-10" >
          <label for="un" class="col-sm-2 control-label">Name:</label>
          <input type="text" id="un" name="pets_name" class="form-control" placeholder="Pets Name">
        </div>
      </div>

          <div class="form-group row">
            <div class="col-sm-10">
                <label for="em" class="col-sm-2 control-label">Breed:</label>
                <input type="text" id="em" name="breed_name" class="form-control" placeholder="Breed Type or Name">
            </div>
          </div>


          <div class="form-group row">
            <div class="col-sm-10">
              <label class="col-sm-6" for="fix">Gender:</label>
              <select name="fixed" id="fixed" class="col-sm-5">
                <option value="0"> Gender </option>
                <option value="1"> Male </option>
                <option value="2"> Female </option>
              </select>
            </div>
          </div>

          <div class="form-group row">
              <div class="col-sm-12">
                <label class="col-sm-2" for="fix">Age:</label>
                <select name="fixed" id="fixed" class="col-sm-2">
                  <option value="0"> # </option>
                  <option value="1"> 1 </option>
                  <option value="2"> 2 </option>
                  <option value="3"> 3 </option>
                  <option value="4"> 4 </option>
                  <option value="5"> 5 </option>
                  <option value="6"> 6 </option>
                  <option value="7"> 7 </option>
                  <option value="8"> 8 </option>
                  <option value="9"> 9 </option>
                  <option value="10"> 10 </option>
                  <option value="11"> 11 </option>
                  <option value="12"> 12 </option>
                  <option value="12"> 13 </option>
                  <option value="14"> 14 </option>
                  <option value="15"> 15+ </option>
                </select>
                <select name="fixed" id="fixed" class="col-sm-5">
                  <option value="0"> Week/Months/Years </option>
                  <option value="1"> Weeks </option>
                  <option value="1"> Months </option>
                  <option value="2"> Years </option>
                </select>
              </div>
            </div>

        <div class="form-group row">
            <div class="col-sm-10">
              <label class="col-sm-6" for="fix">Spay-Neuter:</label>
              <select name="fixed" id="fixed" class="col-sm-5">
                <option value="0"> Please Select </option>
                <option value="1"> Yes </option>
                <option value="2"> NO </option>
                <option value="3"> Unknown </option>
              </select>
            </div>
          </div>

          <div class="form-group row">
            <div class="col-sm-10">
              <label class="col-sm-6" for="shot">Shots Current:</label>
              <select name="shots" id="shots" class="col-sm-5">
                <option value="0"> Please Select </option>
                <option value="1"> Yes </option>
                <option value="2"> NO </option>
                <option value="3"> Unknown </option>
              </select>
            </div>
          </div>

          <div class="form-group row">
            <div class="col-sm-10">
              <label class="col-sm-6" for="kid">Kid-Friendly:</label>
              <select name="kid_friendly" id="kid_friendly" class="col-sm-5">
                <option value="0"> Please Select </option>
                <option value="1"> Yes </option>
                <option value="2"> NO </option>
                <option value="3"> Unknown </option>
              </select>
            </div>
          </div>

